{
  "name": "alerter",
  "description": "Send twilio sms messages from command line",
  "version": "1.0.0",
  "author": "Dave Carroll @dcarroll",
  "bugs": "https://github.com/dcarroll/alerter/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^5.0.0",
    "@salesforce/core": "^3.32.12",
    "@slack/web-api": "^5.0.0",
    "@slack/webhook": "^5.0.0",
    "@types/twilio": "0.0.10",
    "tslib": "^1",
    "twilio": "^4.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.5.0",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^11.13.4",
    "chai": "^4",
    "globby": "^10.0.0",
    "mocha": "^6.1.2",
    "nyc": "^14.1.0",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "~3.4.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/dcarroll/alerter",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "alerter": {
        "description": "Commands to say alert via twilio."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "dcarroll/alerter",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
